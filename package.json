{
  "name": "crosx",
  "description": "iwgx personal blog",
  "version": "1.0.0",
  "author": "Ilham Wahabi <iwgx.github.io>",
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^14.1.1",
    "@contentful/rich-text-types": "^14.1.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "dayjs": "^1.8.34",
    "gatsby": "2.24.47",
    "gatsby-plugin-manifest": "2.4.23",
    "gatsby-plugin-offline": "3.2.23",
    "gatsby-plugin-postcss": "2.3.11",
    "gatsby-plugin-purgecss": "5.0.0",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-typescript": "^2.4.18",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "graphql": "15.3.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .tsx --ext .ts --ext .js .",
    "analyze:prettier": "prettier --check **/*.{tsx,ts,js,css,md,json}",
    "analyze": "yarn analyze:lint && yarn analyze:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "fix:lint": "eslint --ext .tsx --ext ts --ext .js . --fix",
    "fix:prettier": "prettier --write **/*.{tsx,ts,js,css,md,json}",
    "fix": "yarn fix:lint && yarn fix:prettier",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,js,css,md,json}": [
      "yarn fix",
      "git add ."
    ]
  },
  "devDependencies": {
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "babel-eslint": "10.1.0",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.0",
    "gatsby-source-contentful": "^2.3.37",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.7.2",
    "typescript": "3.9.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iwgx/crosx"
  },
  "bugs": {
    "url": "https://github.com/iwgx/crosx/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
