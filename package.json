{
  "name": "ilham-wahabi",
  "description": "personal website by iwgx",
  "version": "1.0.0",
  "author": "Ilham Wahabi <iwgx.io>",
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^14.1.1",
    "@contentful/rich-text-types": "^14.1.1",
    "dayjs": "^1.10.4",
    "gatsby": "^3.1.2",
    "gatsby-plugin-manifest": "^3.1.0",
    "gatsby-plugin-offline": "^4.1.0",
    "gatsby-plugin-react-helmet": "^4.1.0",
    "gatsby-plugin-splitbee": "^0.1.0",
    "gatsby-plugin-typescript": "^3.1.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "graphql": "15.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .tsx --ext .ts --ext .js .",
    "analyze:prettier": "prettier --check **/*.{tsx,ts,js,css,md,json}",
    "analyze": "yarn analyze:lint && yarn analyze:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "fix:lint": "eslint --ext .tsx --ext ts --ext .js . --fix",
    "fix:prettier": "prettier --write **/*.{tsx,ts,js,css,md,json}",
    "fix": "yarn fix:lint && yarn fix:prettier",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,js,css,md,json}": [
      "yarn fix",
      "git add ."
    ]
  },
  "devDependencies": {
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "10.1.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "gatsby-plugin-postcss": "^4.1.0",
    "gatsby-source-contentful": "^2.3.37",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "postcss": "^8.2.8",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.0.4",
    "typescript": "^4.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iwgx/profile"
  },
  "bugs": {
    "url": "https://github.com/iwgx/profile/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
