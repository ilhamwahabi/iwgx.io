{
  "name": "crosx",
  "description": "Iwgx personal blog",
  "version": "1.0.0",
  "author": "Ilham Wahabi <iwgx.github.io>",
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^13.4.0",
    "@contentful/rich-text-types": "^14.1.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "dayjs": "^1.8.21",
    "gatsby": "2.19.23",
    "gatsby-plugin-manifest": "2.2.42",
    "gatsby-plugin-offline": "3.0.35",
    "gatsby-plugin-postcss": "2.1.20",
    "gatsby-plugin-purgecss": "4.0.1",
    "gatsby-plugin-react-helmet": "3.1.22",
    "gatsby-plugin-typescript": "^2.1.27",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "graphql": "14.6.0",
    "prop-types": "15.7.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-helmet": "5.2.1"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .tsx --ext .ts --ext .js .",
    "analyze:prettier": "prettier --check '**/*.{tsx,ts,js,css,md,json}'",
    "analyze": "yarn analyze:lint && yarn analyze:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write '**/*.{js,css,md,json}'",
    "fix": "yarn fix:lint && yarn fix:prettier",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,js,css,md,json}": [
      "yarn fix",
      "git add ."
    ]
  },
  "devDependencies": {
    "@types/node": "^13.7.6",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "5.2.0",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.5.0",
    "gatsby-source-contentful": "^2.1.89",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "tailwindcss": "1.2.0",
    "typescript": "3.8.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iwgx/crosx"
  },
  "bugs": {
    "url": "https://github.com/iwgx/crosx/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
