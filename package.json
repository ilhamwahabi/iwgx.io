{
  "name": "crosx",
  "description": "Iwgx personal blog",
  "version": "1.0.0",
  "author": "Ilham Wahabi <iwgx.github.io>",
  "dependencies": {
    "gatsby": "2.13.41",
    "gatsby-plugin-manifest": "2.2.4",
    "gatsby-plugin-offline": "2.2.4",
    "gatsby-plugin-postcss": "2.1.2",
    "gatsby-plugin-purgecss": "4.0.0",
    "gatsby-plugin-react-helmet": "3.1.2",
    "gatsby-plugin-typescript": "^2.1.2",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "graphql": "14.4.2",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .tsx --ext .ts --ext .js .",
    "analyze:prettier": "prettier --check '**/*.{tsx,ts,js,css,md,json}'",
    "analyze": "yarn analyze:lint && yarn analyze:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write '**/*.{js,css,md,json}'",
    "fix": "yarn fix:lint && yarn fix:prettier",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,js,css,md,json}": [
      "yarn fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/react-helmet": "^5.0.8",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "babel-eslint": "10.0.2",
    "eslint": "5.16.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "4.0.1",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.6.1",
    "gatsby-source-contentful": "^2.1.15",
    "husky": "^3.0.2",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "tailwindcss": "1.0.5",
    "typescript": "3.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iwgx/crosx"
  },
  "bugs": {
    "url": "https://github.com/iwgx/crosx/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
